{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/dsa-buddy-server/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/dsa-buddy-server/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/dsa-buddy-server",
  "projects": {
    "dsa-buddy-server": {
      "type": "application",
      "root": "apps/dsa-buddy-server",
      "entryFile": "main",
      "sourceRoot": "apps/dsa-buddy-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dsa-buddy-server/tsconfig.app.json"
      }
    },
    "code_exec_service": {
      "type": "application",
      "root": "apps/code_exec_service",
      "entryFile": "main",
      "sourceRoot": "apps/code_exec_service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/code_exec_service/tsconfig.app.json"
      }
    }
  }
}